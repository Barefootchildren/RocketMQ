<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.coder.mapper.OrderMapper">

    <insert id="insert" keyColumn="id" keyProperty="id" parameterType="com.coder.entity.Order" useGeneratedKeys="true">
        insert into `order_records` (user_id, goods_id, create_time)
        values (#{userid,jdbcType=INTEGER}, #{goodsid,jdbcType=INTEGER}, #{createtime,jdbcType=TIMESTAMP})
    </insert>
</mapper>